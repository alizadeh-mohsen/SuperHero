{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console"  ],
    "MinimumLevel": "Warning",
    "WriteTo": [
      { "Name": "Console" },
      //{
      //  "Name": "File",
      //  "Args": {
      //    "path": "D://logs/log-.txt",
      //    "rollingInterval": "Day"
      //  }
      //},
      {
        "Name": "Seq",
        "Args": {
          //"serverUrl": "http://host.docker.internal:5341"
          "serverUrl": "http://superhero.log"
        }

      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithProcessId" ],
    "Properties": {
      "Application": "SuperHero.API"
    }
  },
  "AllowedHosts": "*",
  "BaseApiUrl": "https://api.hero.com:44310"
}
